{
  "name": "@vouch-protocol/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Vouch Protocol - communicate with the Vouch Daemon",
  "keywords": [
    "vouch",
    "vouch-protocol",
    "signing",
    "ed25519",
    "c2pa",
    "content-credentials",
    "provenance",
    "identity",
    "cryptography"
  ],
  "author": "Ramprasad Anandam Gaddam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vouch-protocol/vouch.git",
    "directory": "packages/sdk-ts"
  },
  "homepage": "https://vouch-protocol.com",
  "bugs": {
    "url": "https://github.com/vouch-protocol/vouch/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "sideEffects": false
}
